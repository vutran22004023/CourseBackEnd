{
    "version": 2,
    "builds": [
    {
      "src": "./src/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "./firebase-cert.json"
        ]
      }
    }
  ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/server.js"
        }
    ]
}