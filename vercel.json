{
    "version": 2,
    "builds": [
      {
        "src": "./vercel-server.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "./firebase-cert.json"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/vercel-server.js"
      }
    ]
  }